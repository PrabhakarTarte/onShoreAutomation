<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests" thread-count="5">
	<test name="GRID TEST WITH SERIAL EXECTION WITH BROWSER Chrome">
		<parameter name="browserType" value="chrome" />
		<classes>
			<class name="com.tarte.script.test"/>
		</classes>
	</test>
	<test name="GRID TEST WITH SERIAL EXECTION WITH BROWSER Firefox">
		<parameter name="browserType" value="firefox" />
		<classes>
			<class name="com.tarte.script.test"/>
		</classes>
	</test>
	<test name="GRID TEST WITH SERIAL EXECTION WITH BROWSER IE">
		<parameter name="browserType" value="edge" />
		<classes>
			<class name="com.tarte.script.test"/>
		</classes>
</test> <!-- Test -->
</suite> <!-- Suite -->
